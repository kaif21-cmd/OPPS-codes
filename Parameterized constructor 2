#include<iostream>
using namespace std;
class Employee{
    public:
    int id;
    string name;
    float salary;
    Employee(int i,string n,float s){
        id=i;
        name=n;
        salary=s;
    }
    void display(){
        cout<<id<<" "<<name<<" "<<salary<<endl;
    }
};
int main()
{ 
    cout<<"id"<<" "<<"name"<<" "<<"salary"<<endl;
    Employee e1(210,"kaif",90000);
    Employee e2(211,"zara",950000);
    Employee e3(212,"srk",100000);
    e1.display();
    e2.display();
    e3.display();
}
